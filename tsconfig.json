{
  "extends": "./.svelte-kit/tsconfig.json",
  "compilerOptions": {
    "types": ["bun-types"],
    "allowJs": true,
    "checkJs": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "noImplicitAny": true,
    "lib": ["ES2021"]
  }
}
